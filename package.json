{
  "name": "office-toolbox",
  "version": "1.0.0",
  "description": "Tools for Office Add-ins",
  "main": "./app/util.js",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf app && concurrently \"npm run tsc\"",
    "start": "rimraf app && concurrently \"npm run tsc-watch\"",
    "validate-office-addin": "validate-office-addin"
  },
  "keywords": [
    "office"
  ],
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-toolbox": "./app/office-toolbox.js"
  },
  "files": [
    "templates"
  ],
  "dependencies": {
    "bower": "^1.8.0",
    "chalk": "^2.0.1",
    "commander": "^2.9.0",
    "fs-extra": "^3.0.1",
    "generator-office": "^1.1.5",
    "inquirer": "^3.1.0",
    "jszip": "^3.1.3",
    "junk": "^2.1.0",
    "office-addin-validator": "^1.0.5",
    "open": "^0.0.5",
    "xml2js": "^0.4.17",
    "yo": "^2.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.3.31",
    "concurrently": "^3.5.0",
    "node-powershell": "^3.1.1",
    "node-status": "^1.0.0",
    "rimraf": "^2.6.1",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.2"
  }
}
